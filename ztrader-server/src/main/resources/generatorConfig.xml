<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <context id="database" targetRuntime="MyBatis3Simple">
        <property name="javaFileEncoding" value="UTF-8"/>
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="true"/>
            <property name="addRemarkComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="org.h2.Driver"
                        connectionURL="jdbc:h2:file:~/.ztrader/ztrader;AUTO_SERVER=TRUE"
                        userId="sa"
                        password=""/>

        <javaModelGenerator targetPackage="org.zenoda.ztrader.data.domain" targetProject="src/main/java"/>
        <sqlMapGenerator targetPackage="org.zenoda.ztrader.data.mapper" targetProject="src/main/resources"/>
        <javaClientGenerator targetPackage="org.zenoda.ztrader.data.mapper" targetProject="src/main/java"
                             type="XMLMAPPER"/>

        <table tableName="zt_data_category" mapperName="DataCategoryMapper" domainObjectName="DataCategory"
               enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" selectByExampleQueryId="false"/>
        <table tableName="zt_company" mapperName="CompanyMapper" domainObjectName="Company"
               enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" selectByExampleQueryId="false"/>
        <table tableName="zt_trade" mapperName="TradeMapper" domainObjectName="Trade"
               enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" selectByExampleQueryId="false"/>
    </context>
</generatorConfiguration>
